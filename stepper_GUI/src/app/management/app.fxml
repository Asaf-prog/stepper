<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox fx:id="appbox" maxHeight="960.0" maxWidth="1440.0" prefHeight="720.0" prefWidth="1080.0" style="-fx-alignment: CENTER;" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.management.mainController">
   <children>
      <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="720.0" prefWidth="1080.0" style="-fx-hbar-policy: AS_NEEDED; -fx-vbar-policy: AS_NEEDED; -fx-background-color: #36394e;" HBox.hgrow="ALWAYS">
