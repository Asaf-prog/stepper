                  <VBox prefHeight="720.0" prefWidth="1080.0" style="-fx-alignment: CENTER;" VBox.vgrow="ALWAYS">
                     <children>
                        <fx:include fx:id="headerComponent" source="../header/header.fxml" VBox.vgrow="ALWAYS" />
